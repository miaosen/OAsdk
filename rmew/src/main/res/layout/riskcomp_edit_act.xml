<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">
    <com.oaui.view.HeaderView
        android:id="@+id/headerView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:leftText="返回"
        app:title="企业预警详细信息">

    </com.oaui.view.HeaderView>


    <LinearLayout
        android:id="@+id/ln_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="企业名称:"
            />
            <EditText
                android:id="@+id/COMPANY_NAME"
                style="@style/ed_edit"
            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="许可证编号:"

            />
            <EditText
                android:id="@+id/LICENSE_NO"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="企业类型:"

            />
            <EditText
                android:id="@+id/COMPANY_TYPE"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="经营范围:"

            />
            <EditText
                android:id="@+id/BUSINESS_SCOPE"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="经营地址:"

            />
            <EditText
                android:id="@+id/MANA_ADDRESS"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="仓库地址:"

            />
            <EditText
                android:id="@+id/WAREHOUSE_ADDRESS"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="法定代表人:"

            />
            <EditText
                android:id="@+id/LEGAL_REPRESENTATIVE"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="企业负责人:"

            />
            <EditText
                android:id="@+id/RESPONSIBLE_PERSON"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="质量负责人:"

            />
            <EditText
                android:id="@+id/QUALITY_LEADER"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="行政区划（市）:"

            />
            <EditText
                android:id="@+id/CITY"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="行政区划（区）:"

            />
            <EditText
                android:id="@+id/DISTRICT"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="行政区划（镇街）:"

            />
            <EditText
                android:id="@+id/STREET"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="有效截止日期:"

            />
            <EditText
                android:id="@+id/EXPIRE_DATE"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="发证日期:"

            />
            <EditText
                android:id="@+id/CER_DATE"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="发证机关:"

            />
            <EditText
                android:id="@+id/ISSUING_ORGAN"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="社会信用代码（身份证号码）:"

            />
            <EditText
                android:id="@+id/CREDIT_ID"
                style="@style/ed_edit"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                style="@style/tv_edit"
                android:text="组织机构代码:"

            />
            <EditText
                android:id="@+id/ORG_CODE"
                style="@style/ed_edit"

            />
        </LinearLayout>
    </LinearLayout>
    <com.oaui.view.TailView
        android:id="@+id/tailView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:valueText="确定">


    </com.oaui.view.TailView>
</LinearLayout>